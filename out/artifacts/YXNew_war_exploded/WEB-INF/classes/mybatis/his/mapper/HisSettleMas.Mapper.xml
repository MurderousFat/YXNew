            ake007=#{ake007},
            aae013=#{aae013},
            akc090=#{akc090},
            akc220=#{akc220},
            aab014=#{aab014},
            akc264=#{akc264},
            akc190=#{akc190},
            jbrmc=#{jbrmc},
            ckz543=#{ckz543},
            aae011=#{aae011},
            sessionid=#{sessionid},
            jmc=#{jmc},
            his_bill_state=#{hisBillState},
            merchant_id=#{merchantId},
            aaz217=#{aaz217},
            ykc675=#{ykc675},
            ykc618=#{ykc618},
            aac004=#{aac004},
            person_name=#{personName},
            settle_type_flag=#{settleTypeFlag},
            self_pay=#{selfPay},
            xyb_order_no=#{xybOrderNo},
            </set>
        <where>
            ID = #{id} 
        </where>
    </update>
    
    <sql id="leftjoin_sql">
   	left join bi_merchant as m on m.id=t.merchant_id 
